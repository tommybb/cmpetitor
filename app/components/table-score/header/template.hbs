{{yield}}
<tr>
  <th>
    {{input value=dateFilterValue}}
  </th>
  <th>
    {{input type='number' value=result}}
  </th>
  <th>
    {{input value=sportFilterValue}}
    {{#link-to 'scores' (query-params sport=sportFilterValue) class="button"}}✔{{/link-to}}
  </th>
  <th>
    {{input value=classroomFilterValue}}
    {{#link-to 'scores' (query-params classroom=classroomFilterValue) class="button"}}✔{{/link-to}}
  </th>
  <th>
    {{input value=pupilFilterValue}}
    {{#link-to 'scores' (query-params pupil=pupilFilterValue) class="button"}}✔{{/link-to}}
  </th>
</tr>
<tr>
  <th>
    Date
  </th>
  <th>
    Result
  </th>
  <th>
    Sport
  </th>
  <th>
    Class room
  </th>
  <th>
    Pupil
  </th>
</tr>